package basic;

public class Payload {
	
	public static String BookerApiPayload()
	{
		String Book = "{\r\n"
				+ "    \"firstname\" : \"Jim\",\r\n"
				+ "    \"lastname\" : \"Brown\",\r\n"
				+ "    \"totalprice\" : 111,\r\n"
				+ "    \"depositpaid\" : true,\r\n"
				+ "    \"bookingdates\" : {\r\n"
				+ "        \"checkin\" : \"2018-01-01\",\r\n"
				+ "        \"checkout\" : \"2019-01-01\"\r\n"
				+ "    },\r\n"
				+ "    \"additionalneeds\" : \"Breakfast\"\r\n"
				+ "}";
		return Book ;
		
	}
	public static String EmployeePayload() 
	{
		String Emp= "{\r\n"
				+ "\"name\": \"morpheus\",\r\n"
				+ "\"job\": \"leader\"\r\n"
				+ "}";
				
		return Emp;
		
		
	}

	

}
